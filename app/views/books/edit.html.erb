<h1>Editing book</h1>
<%= form_with model: @book do |f| %> <!-- 7章にて-->
  <div class="form">Title </div>
    <%= f.text_field :title %>
  
  <div class="form">Body</div>
  <%= f.text_area :body %>

  <br>
  <%= f.submit 'Update Book' %>

<% end %>

<%= link_to "Show",@book %>
<%= link_to "Back",books_path %>